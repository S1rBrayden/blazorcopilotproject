@foreach (var evt in events)
{
    <EventCard Event="evt" />
}

@code {
    private List<EventModel> events = new()
    {
        new EventModel { Name = "Corporate Gala", Date = DateTime.Today.AddDays(7), Location = "Calgary" },
        new EventModel { Name = "Team Retreat", Date = DateTime.Today.AddDays(30), Location = "Banff" }
    };
}
